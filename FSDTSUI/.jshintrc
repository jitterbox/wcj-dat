{

  "maxerr" : 50, // {int} Maximum error before stopping
  "camelcase" : true, // true: Identifiers must be in camelCase
  "indent": 2 , //{int} Number of spaces to use for indentation
  "white" : true,
  "multistr" : false,
  "quotmark" : "single",
  "maxlen" : 160, //{int} Max number of characters per line
  "trailing" : true,
  "curly" : true, // true: Require {} for every new block or scope
  "eqeqeq": true, //prohibits the use of == and !=, instead developers should use === and !===.
  "strict" : true, // true: Requires all functions run in ES5 Strict Mode
  "noempty" : true, // true: Prohibit use of empty blocks
  "undef" : true, // true: Require all non-global variables to be declared (prevents global leaks)
  "unused" : true, // true: Require all defined variables be used
  "maxparams" : 5, // {int} Max number of formal params allowed per function
  "maxdepth" : 10, // {int} Max depth of nested blocks (within functions)
  "maxstatements" : 50, // {int} Max number statements per function
  "maxcomplexity" : 10, // {int} Max cyclomatic complexity per function
  // Development.
  "debug"         : false,  //True , Allow debugger statements e.g. browser breakpoints.
  "devel"         : true,   // Allow developments statements e.g. `console.log();`.
  "globalstrict": true,
  "globals" : {
   "fsdtsApp":true,
   "console": false,
    "$"                    : false,
    "angular"              : false,
    "browser"              : false,
    "repeater"             : false,
    "element"              : false,
    "inject"               : false,
    "afterEach"            : false,
    "beforeEach"           : false,
    "confirm"              : false,
    "context"              : false,
    "describe"             : false,
    "expect"               : false,
    "it"                   : false,
    "jasmine"              : false,
    "JSHINT"               : false,
    "mostRecentAjaxRequest": false,
    "qq"                   : false,
    "runs"                 : false,
    "spyOn"                : false,
    "spyOnEvent"           : false,
    "waitsFor"             : false,
    "xdescribe"            : false
  }

}